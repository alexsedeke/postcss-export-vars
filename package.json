{
  "name": "postcss-export-vars",
  "version": "0.1.0",
  "description": "PostCSS plugin to export variables as JS constants or JSON.",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "javascript",
    "vars",
    "export",
    "constant"
  ],
  "author": "Jan Nahody <jan.nahody@gmail.com>",
  "license": "MIT",
  "repository": "nahody/postcss-export-vars",
  "bugs": {
    "url": "https://github.com/nahody/postcss-export-vars/issues"
  },
  "homepage": "https://github.com/nahody/postcss-export-vars",
  "dependencies": {
    "lodash": "^4.10.0",
    "postcss": "^5.0.16"
  },
  "devDependencies": {
    "ava": "^0.12.0",
    "eslint": "^2.1.0",
    "eslint-config-postcss": "^2.0.0"
  },
  "scripts": {
    "test": "ava && eslint *.js"
  },
  "eslintConfig": {
    "extends": "eslint-config-postcss/es5",
    "rules": {
      "max-len": "off",
      "no-unused-vars": "off"
    }
  }
}
